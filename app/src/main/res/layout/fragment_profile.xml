<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.profile.ProfileFragment">

    <!-- TODO: Update blank fragment layout -->

    <!--364 712-->
    <!--
    app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        -->
    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="42dp"
        android:layout_marginTop="42dp"
        android:lineSpacingExtra="12sp"
        android:text="프로필"
        android:textColor="#3c3c3c"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageButton
        android:id="@+id/btn_alarm"
        android:layout_width="26.4dp"
        android:layout_height="31.1dp"

        android:layout_marginTop="47.4dp"
        android:layout_marginEnd="42dp"
        android:background="#00000000"
        android:src="@drawable/white_img"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btn_profileImage"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="159dp"
        android:background="#00000000"
        android:src="@drawable/profile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <!--찜목록, 내가쓴글, 보낸신청서, 받은신청서 -->

    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="188dp"
        android:layout_marginEnd="100dp"
        android:text="전주대"
        android:textColor="#3c3c3c"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_profileEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="75dp"
        android:text="프로필 편집"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="389.5dp"
        android:src="@drawable/icon_love_outline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_loveIt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="찜 목록"
        android:textColor="#3c3c3c"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_marginTop="386dp"
        android:layout_marginStart="103dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/font_awsome_edit"
        android:layout_marginStart="56dp"
        android:layout_marginTop="458.5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/text_writePost"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="내가 쓴 글"
        android:textColor="#3c3c3c"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginTop="456dp"
        android:layout_marginStart="103dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:src="@drawable/font_awsome_paper_plane"
        android:layout_marginTop="529dp"
        android:layout_marginStart="56dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/text_sendApplicationForm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="보낸 신청서"
        android:textColor="#3c3c3c"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginTop="529dp"
        android:layout_marginStart="103dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/font_awsome_envelope_open_text"
        android:layout_marginTop="598dp"
        android:layout_marginStart="56dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/text_receivedApplicationForm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="받은 신청서"
        android:textColor="#3c3c3c"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginTop="596dp"
        android:layout_marginStart="103dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />


    <!--로그아웃, 회원탈퇴 -->

    <TextView
        android:id="@+id/text_signout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="98dp"
        android:layout_marginTop="762dp"
        android:text="로그아웃"
        android:textColor="#939393"
        android:textSize="18sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_membershipWithdrawal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="268dp"
        android:layout_marginTop="762dp"
        android:text="회원탈퇴"
        android:textColor="#939393"
        android:textSize="18sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>