<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <TextView
        android:layout_width="352dp"
        android:layout_height="68dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="611dp"
        android:layout_marginEnd="39dp"
        android:gravity="center_horizontal"
        android:lineSpacingExtra="14sp"
        android:text="당신은 테레사 수녀와 같은"
        android:textColor="#3c3c3c"
        android:textSize="30sp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="56dp"
        android:layout_marginTop="667dp"
        android:layout_marginEnd="60dp"
        android:gravity="center_horizontal"
        android:lineSpacingExtra="14sp"
        android:text="너그러운 품성의 소유자!"
        android:textColor="#1c7eff"
        android:textSize="30sp"
        android:textStyle="bold" />

    <android.widget.Button
        android:id="@+id/button_check2"
        android:layout_width="344dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="42dp"
        android:layout_marginLeft="42dp"
        android:layout_marginTop="579dp"
        android:layout_marginEnd="42dp"
        android:layout_marginBottom="134dp"
        android:background="@drawable/btn_round_blue_box"
        android:text="확인"
        android:textColor="#ffffff"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="428dp"
        android:layout_height="465dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="0dp"
        app:srcCompat="@drawable/bible_teaching_cuate" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView2"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="42dp"
        android:layout_marginBottom="357dp"
        android:text="INFJ"
        android:textSize="20sp" />

</RelativeLayout>